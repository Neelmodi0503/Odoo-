<odoo>
    <template id = "report_patient">
        <t t-call="web.external_layout">
            <t t-foreach = "docs" t-as="o">
                <div class="page">
                    <h2 class = "report-title d-flex justify-content-center mt-5 " >
                        Patient Report </h2>
                    <p>
                        <strong> Name:- </strong>
                        <t t-esc='o.name' />
                        </br>
                        <strong> Age :- </strong>
                        <t t-esc="o.age" />
                        </br>
                        <strong> Date Of Birth</strong>
                        <t t-esc='o.date_of_birth' />
                    </P>

                </div>


                <div> 
                    <h2 class="selection-title d-flex justify-cotent-center" />
                        <table>
                            <tr>
                                <th> Shipping Price </th>
                                <th> Tracking Id </th>
                                <th> Status </th>
                            </tr>

                            <tr>
                                <td> <t t-esc="o.shipping_price" />
                                <td> <t t-esc= "o.tracking_number" />
                                <td> <t t-esc="o.shipping_status" />
                            </tr>
                        </table>
                </div>
            </t>
    
                        


